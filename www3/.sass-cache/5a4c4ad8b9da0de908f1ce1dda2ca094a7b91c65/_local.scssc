o:Sass::Tree::RootNode
:@template",$headerHeight:130px;
$powerSlideHeight:300px;
$hoverOn:#0a0;

/* core structure */
body {
    background:#000;
    background-image:url("../img/texture.jpg");
    background-repeat:repeat-x;
    font-size:1em;
    letter-spacing:0em;
    font-family:serif;
    font-weight:lighter;
    color:#fff;
}
a,
a:link,
a:visited {
    color:#fff;	     
}
a.navon,
a:hover {
    color:$hoverOn;
    text-decoration:none;
}

#footer-container {
    position:relative;
    border-top:1px solid #fff;
    opacity:.3;
    padding:50px;
    width:100%;
    max-width:1200px;
    height:200px;
    margin-top:100%;
    text-align:center;
    margin:0 auto;
    font-family:"MS Trebuchet",tahoma,helvetica;
    font-size:.9em;
}
.foreground-container {
    z-index:1;
    width:90%;
    padding-left:5%;
    padding-right:5%;
    
    &.grey {
	background-color:#aaa;
	padding-top:100px;
	height:500px;
	h1 {
	    color:#000;
	    font-size:4em;
	    text-align:center;
	}
    }

    &.white {
	background-color:#fff;
	padding-top:100px;
	height:500px;
	h1 {
	    color:#000;
	    font-size:4em;
	    text-align:center;
	}
    }

    &.pink {
	background-color:#f0f;
	padding-top:100px;
	height:500px;
	h1 {
	    color:#000;
	    font-size:4em;
	    text-align:center;
	}
    }
}

.foreground {
    position:relative;
    width:100%;
    max-width:1200px;
    margin:0 auto;
    padding-bottom:30px;

    &.origins {
	background-image: url("../img/origins.png");
	background-repeat:no-repeat;
	background-position:right center;
    }

    &.mission {
	background-image: url("../img/mission.png");
	background-repeat:no-repeat;
	background-position:right bottom;
    }
    &.approach {
	background-image: url("../img/approach.png");
	background-repeat:no-repeat;
	background-position:right bottom;
    }

}

#header-container {
    position:relative;
    height:$headerHeight;
    max-width:1200px;
    width:100%;
    margin:0 auto;
}

#header-stripe {
    position:absolute;
    width:100%;
    height:$headerHeight;
    background:#000;
    opacity:.3;
    z-index:2;
}

#main-content {
    margin-top:30px;
    position:relative;
    width:100%;
    height:100%;
}

/* MEDIA FEED */

.media-feed-box {
    .tweet-divider {
	border-bottom:1px dotted #555;
	margin-bottom:20px;
	margin-top:20px;
    }

    p {
	min-height:70px;
	clear:both;
    }
    .icon-logo {
	width:50px;
	margin:20px;
	margin-left:0px;
	margin-top:0px;
	float:left;
	border:1px solid #fff;
    }
}
.media-feed-title {

    position:relative;
    height:40px;
    top:-40px;
    vertical-align:bottom;

    .media-feed-icon {
	margin-left:10px;
    }
    h1 {
	position:relative;
	top:-24px;
	font-size:1.4em;
	font-style:italic;
	text-align:right;
	color:#555;

    }
    
}
h12 {
    font-size:2em;
    position:relative;
    top:-40px;
    left:-10px;
    height:40px;
    padding-top:6px;
    background-repeat:no-repeat;
    background-position:center top;
    padding-left:100px;

}
.media-feed-box {
    p {
	font-size: 1em;
	line-height: 1.3em;
	font-family: "helvetica";
    }
    
}
.content-container {
    width:90%;
    position:relative;
    padding-left:5%;
    padding-right:5%;
    &.first {
	padding-left:0%;
	padding-right:10%;
    }
    &.last {
	padding-left:10%;
	padding-right:0%;
    }
}
.content-box-background {
    position:absolute;
    opacity:.6;
    z-index:1;
    width:90%;
    height:100%;
    background:#000;
    border-top:2px dotted #ccc;
}

.content-box-foreground {
    position:relative;
    margin:5%;
    z-index:2;
    color:#fff;
    padding-bottom:100px;
    .title {
	font-size:3em;
    } 
    p {
	color:#aaa;
    }
}


/* NAVIGATION  & NEWSLETTER SIGNUP */

#newsletter-signup-container {
    position:absolute;
    font-family:'Helvetica Neue',Helvetica,Arial,Sans-Serif;
    font-size:.6em;
    font-weight:bold;
    width:100%;
    top:10px;
    left:-10px;
  	
    input {
	font-size:.7em;
	width:260px;
	border:1px solid #666;
	background:#333;
	color:#888;
	padding:5px;
	height:18px;
	position:relative;
	top:10px;
    }

    button {
	position:relative;
	top:10px;
	font-size:.7em;
	background:#444;
	border:1px solid #888;
	height:30px;
	padding:5px;
	color:#aaa;
    }
}

#newsletter-signup {
    border:2px dotted #444;
    float:right;
    height:50px;
    width:380px;
    text-align:center;
}

#navigation-container {
    width:100%;
    height:41px;
    z-index:2;
    font-size:1.7em;
    position:absolute;
    text-align:right;
    font-weight:lighter;
    letter-spacing:1px;

    a {
	height:20px;
	@include transition(color .6s);
	text-decoration:none;
    }
    a,
    a:link,
    a:visited {
	color:#fff;	     
    }
    a.navon,
    a:hover {
	color:$hoverOn;
	text-decoration:none;
    }

    .main-navigation {
	position:absolute;
	top:84px;
	width:100%;
	list-style:none;
    }

    li {
	display:inline-block;
	padding-right:40px;
    }
}


/* LOGO */
#logo-container {
    z-index:10;
    position:absolute;
    margin-top:5px;
    left:0px;
    width:550px;
    height:150px;
}

.logo {
    position:absolute;
    left:10px;
    top:25px;
    font-family:"Trebuchet MS";
    font-size:1.1em;
    font-weight:bold;
    color:#fff;
    letter-spacing:0em;
    z-index:3;
    opacity:1;

}
.logo-201 {
    font-size:1.9em;
    position:relative;
    top:-38px;
    left:2px; 
}
.logo-digital {
    font-size:3.9em;
    position:relative;
    left:-10px;
}
.logo-co-uk {
    font-size:1.2em;
    position:relative;
    left:-10px;
}
.logo-marketing {
    color:#f0f;
    font-size:1.1em;
    position:relative;
    left:-168px;
    font-weight:normal;
    letter-spacing:0em;
    top:18px;
    @include text-shadow(none);
}

.logo-co-uk,
.logo-201,
.logo-digital {
    @include text-shadow(0px 0px 20px #f0f);
}

.logo-marketing {
    color:#f0f;
}


/* POWER SLIDES */


#power-content {
    overflow:hidden;
    position:relative;
    margin-top:30px;
    width:100%;
    min-height:$powerSlideHeight;
    font-family:serif;     
    
}

.slide {
    z-index:2;
    position:absolute;
    top:-400px;
    opacity:.9;
    width:100%;
    min-height:$powerSlideHeight;
    font-style:italic;

    .title {
	font-size:6em;
    }
    
    .subtitle {
	font-size:3em;
    }

    &.centerslide {
	
	text-align:center;
	padding-top:80px;
    }

    &.fromright {
	top:0px;
	left:100%;
    }
}
#paused-slide-container {
    z-index:10;
    position:absolute;
    width:100%;
    min-height:$powerSlideHeight;
}
.slide-background {
    position:absolute;
    top:-400px;
    height:$powerSlideHeight;
    width:100%;
    background:#000;
    opacity:.3;
    z-index:1;
}
.slide-foreground {
    width:100%;
    height:$powerSlideHeight;
    position:absolute;
    padding:20px;
    z-index:2;
    top:-300px;
}
.slide-content {

    padding:50px;
    position:relative;
    z-index:2;
    p {

	font-family:"trebuchet MS";
	color:#ccc;
	padding-right:250px;
	padding-top:20px;
	letter-spacing:.1em;
	font-size:1.2em;
	padding-left:20px;
    }
}
.slide-img {
    position:absolute;
    z-index:1;
    width:200px;
    top:40px;
    right:40px;

}
.subslide {
    position:relative;
    z-index:2;

    .title {
	font-size:6em;
	padding-left:20px;
	padding-top:10px;
	@include text-shadow(2px 2px 4px #000);
    }
    .subtitle {
	padding-top:10px;
	padding-left:100px;
	font-size:2em;
	color:#555;

	&.line2 {
	    padding-top:0px;
	    padding-left:120px;
	}
	   
    }
}
.subslide.subslide2 {
    .slide-img {
	z-index:1;
	position:absolute;
	right:0px;
	top:30px;
	width:470px;
    }
    .subtitle {
	padding-left:70px;

	&.line2 {
	    padding-left:180px;
	}

    }
}
.subslide.subslide3 {

    .subtitle {
	
	padding-left:110px;
	
	&.line2 {
	    padding-left:160px;
	}
	
    }
    .slide-img {
	width:230px;
    }
}


.subslide.subslide4 {
    .subtitle {
	padding-left:90px;
	&.line2 {
	    padding-left:120px;
	}
    }	

    .slide-img {
	width:250px;
	right:60px;
    }
}
.subslide.subslide5 {
    .subtitle {
	&.line2 {
	    padding-left:180px;
	}
    }
    .slide-img {
	width:360px;
    }
}
.subslide.subslide6 {
    .subtitle {
	padding-left:80px;
	
	&.line2 {
	    padding-left:160px;
	}
    }
    
    .slide-img {
	top:50px;
	width:301px;
    }
}
.subslide {
    position:absolute;
    top:0px;
    width:100%;
    float:right;
    &.subslide1 {
      	left:0;
    }
    &.subslide2 {
	left:100%;
    }
    &.subslide3 {
	left:200%;
    }
    &.subslide4 {
	left:300%;
    }
    &.subslide5 {
	left:400%;
    }
    &.subslide6 {
	left:500%;
    }
	    
}
#slide-buttons {
    width:100%;
    text-align:right;
    right:70px;
    padding:20px;
    z-index:10;
    opacity:.5;
    font-family:sans-serif;
    font-size:.7em;
    position:relative;
    top:-10px;
    .slide-button {
	cursor:pointer;
	display:inline-block;
	height:8px;
	width:8px;
	padding-left:2px;
	padding-bottom:2px;
	border:2px solid #999;
	@include border-radius(5px);
	
	&.active,
	&:hover
	{
	    background-color:#fff;
	}
    }

    .pause-button {
	text-align:center;
    }
}
/* CONTENT */
.leadership-item {
    display:inline-block;
    text-align:center;
}
.leadership-photo {
    width:150px;
    height:150px;
    margin:20px;
    border:1px solid #fff;
}
.content-page {
    position:relative;
}
   
.content-page-container {
    position:relative;
    margin:60px;
    margin-top:0px;
    padding-top:60px;
}
.content-page-background {
    position:absolute;
    width:100%;
    height:100%;
    background:#000;
    opacity:.3;
    z-index:1;
    overflow:hidden;
    min-height:400px;
}
.content-page-content {
    min-height:400px;
    width:100%;
    clear:both;
    padding:20px;
    position:relative;
    z-index:2;
    color:#ccc;
    line-height:1.2em;
    font-weight:normal;
    font-family:"Trebuchet MS",helvetica,tahoma;

    .title {
	padding-top:30px;
	color:#fff;
	font-family:serif;
	font-size:4em;
	font-style:italic;
	padding-bottom:60px;

	
    }
    .subtitle {
	padding:10px;
	padding-top:20px;
	padding-bottom:10px;
	font-family:serif;
	font-size:2em;
	color:#fff;
	font-style:italic;

	&.grey {
	    color:#777;
	}
    }
    .emphasised {
	color:#fff;
	font-size:1.2em;
    }
    .content-left {
	width:90%;
	font-size:.9em;
	color:#ccc;
	line-height:1.2em;
	font-weight:normal;
	font-family:"Trebuchet MS",helvetica,tahoma;
	p {
	    padding:10px;
	}
	&.origins {
	    width:60%;
	}
	&.mission {
	    width:60%;
	}
	&.approach {
	    width:50%;
	}
	
    }
}

.slightbox {
    margin:20px;
    border:1px solid #555;
    display:inline-block;
}
.content-navigation {
    margin:15px;
    height:40px;
    position:relative;
    z-index:2;
    font-size:1em;
    color:$hoverOn;
    font-family:"Trebuchet MS",helvetica,tahoma;
    vertical-align:baseline;
    border-bottom:2px dotted #444;
    
    a:link,
    a:visited {
	text-decoration:none;
	color:$hoverOn;
	@include transition(.4s);
    }
    a:hover {
	color:#ff0;
	@include transition(.4s);
    }
		 
    
    li {
	display:inline-block;
	padding-right:10px;
	border-left:2px solid #666;
	padding-left:10px;
	
	&.first {
	    border-left:0px;
	}
	&.active {
	    color:#fff;
	    font-size:2.1em;
	    font-family:serif;
	}
	&.last {
	}
    }
}
.about-img {
    border:1px solid #fff;
    width:250px;
    float:right;
    margin:50px;
    margin-top:0px;
