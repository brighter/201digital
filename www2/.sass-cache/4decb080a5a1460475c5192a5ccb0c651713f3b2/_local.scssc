$headerHeight:130px;

#breadcrumb {
    font-size:.8em;
    padding-top:5px;
    color:#c0c;
    margin-bottom:20px;
    margin-left:50px;
    width:100%;
    max-width:900px;
    
    .trail {
	font-weight:ligher;
	color:#ddd;
    }
}

#power-content {
    overflow:hidden;
    position:relative;
    margin:0 auto;
    max-width:900px;
    height:300px;
    font-style:italic;
    text-align:center;
    font-family:serif;
    .title {
	font-size:6em;
	}
    .subtitle {
	font-size:3em;
    }
    
}

#main-content {
    position:relative;
    margin-top:30px;
    max-width:1200px;
    height:100%;
    border-top:1px dotted #555;
	    
}
#main-content-background {
    position:absolute;
    z-index:1;
    height:100%;
    width:100%;
    background:#000;
    opacity:.3;
}
#main-content-foreground {
    position:absolute;
    padding:40px;
    z-index:2;
    width:100%;
    height:100%;
    color:#fff;

    .title {
	font-size:3em;
    }    
}

#services-navigation-container {
    position:relative;
    z-index:1;
    top:-80px;
    @include border-radius(10px);
    background-color:#303;
    clear:both;
    float:right;
    width:400px;
    left:-10px;
    height:360px;
    padding:20px;
    padding-top:80px;
    border:1px solid #c0c;
    opacity:1;
    @include transition(opacity .6s);
    font-size:.9em;
    p {
	text-align:center;
	margin-bottom:20px;
	font-size:1.2em;
	color:#d0d;
    }
    ul {
	margin:0 auto;
	width:380px;
	height:300px;
    }
    li {
	font-size:.8em;
	float:left;
	text-align:left;
	height:50px;
	width:100%;
	img {
	    float:left;
	    width:40px;
	    margin-top:5px;
	}
	h1 {
	    margin-left:60px;
	    margin-top:20px;
	}
	border-bottom:1px dotted #959;
    }
    
    &.hidden {
	opacity:0;
	top:-600px;
	@include transition(opacity .6s);
    }
}


.padme50 {
    padding:50px;
}
.center {
    text-align:center;
}

.more-info {
    margin-right:10px;
    margin-bottom:10px;
    font-family:$button;
    @include sassy-button("shiny", 5px, 16px, #2b2, #090);
}

#header-stripe {
    position:absolute;
    width:100%;
    height:$headerHeight;
    background:#000;
    opacity:.2;
    z-index:2;
}
#navigation-container {
    font-family: $heading;
    width:100%;
    height:41px;
    max-width:1100px;
    margin:0 auto;
    z-index:2;
    top:80px;
    font-size:1.5em;
    position:absolute;
    text-align:right;

    a {
	height:20px;
	@include transition(color 1s);
    }
    a,
    a:link,
    a:visited {
	color:#fff;	     
    }
    a.navon,
    a:hover {
	color:#f0f;
	text-decoration:none;
    }

    .main-navigation {
	position:relative;
	z-index:2;
	padding-right:100px;
	float:right;
	display:inline-block;
	width:270px;
	list-style:none;
	text-align: justify;
	-ms-text-justify: distribute-all-lines;
	text-justify: distribute-all-lines;
	
	li {
	    display: inline-block;
	    *display: inline;
	    zoom: 1;
	    padding:7px;	    	    
	}

	li.navon {
	    color:#f4f;
	    background-repeat:no-repeat;
	    background-position:center bottom;
	    background-image:url("../img/navigation-pointer.png");
	}

	&:after {   
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}
    }

}


.icon-envelop:before, .icon-phone:before, .icon-facebook:before, .icon-twitter:before {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	padding-right:10px;
	height:40px;
	
}
.icon-envelop:before {
	content: "\e000";
}
.icon-phone:before {
	content: "\e001";
}
.icon-facebook:before {
	content: "\e002";
}
.icon-twitter:before {
	content: "\e003";
}
#contact-container {
    position:absolute;
    text-align:right;
    width:100%;
    top:30px;
    opacity:.8;
    margin-right:0px;

    ul {
	text-align:left;
	display:inline-block;
	list-style:none;
	margin-left:20px;
	li {
	    padding:2px;
	    display:block;
	}
    }
}

body {
    background:#000;
    background-image:url("../img/texture2.jpg");
    background-repeat:repeat-x;
    font-size:1em;
    letter-spacing:0em;
    font-family:$text;
    font-weight:lighter;
    color:#fff;
}

#foreground-container {
    position:absolute;
    z-index:1;
    width:100%;
    height:100%;
}
#foreground {
    position:relative;
    height:100%;
    width:100%;
    max-width:1000px;
    margin:0 auto;
}

.page-container {
    margin:0 auto;
    width:100%;
    margin-top:10px;
}    
#header-container {
    position:relative;
    height:$headerHeight;
    max-width:1100px;
    width:100%;
    margin:0 auto;
}

#media-channels {
    width:900px;
    margin:0px;
    padding:0px;
    line-height:1.3em;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;

    li {
	width:255px;
	display:inline-block;
	zoom:1;
	vertical-align:top;
	text-align:center;
	margin:20px;
	margin-top:0px;
	h1 {
	    font-size:2em;
	    height:30px;
	}
	p {
	    padding-top:20px;
	    text-align:justify;
	    font-weight:lighter;
	    letter-spacing:.1em;
	    font-size:.8em;
	    color:#ddd;
	    }
	
    }
	&:after {   
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}

}
	
#media-content-container {

    list-style:none;
    height:300px;
    margin:0 auto;
    text-align: justify;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;
    padding-top:6px;
    padding-bottom:40px;
    width:100%;
    max-width:1100px;

    li:hover {
	background:#606;
    }	
    li {

	position:relative;
	margin:3px;
	display: inline-block;
	*display: inline;
	zoom: 1;
	min-width:290px;
	width:30%;

	.media-content {
	    position:absolute;
	    opacity:1;
	    background-color:#303;
	    height:270px;
	    @include border-radius(3px);
	    border:1px dotted #f0f;
	    h1 {
		@include border-radius(3px);
		padding:8px;
		background-color:#505;
		text-align:center;
		font-family:$heading;
		color:#f6f;
		font-size:2.5em;
		margin-bottom:10px;
	    }

	    p {
		padding:15px;
		text-align:center;
		font-weight:normal;
		font-family:$text;
		font-size:.8em;
		line-height:1.2em;
		letter-spacing:.1em;
	    }
 	    .media-top {
		height:180px;
	    }
	    .media-bottom {
		position:absolute;
		width:100%;
		bottom:0px;
		padding-top:10px;
		height:50px;
		background-color:#404;
		text-align:center;
		}
	    
	}
    }
	&:after {
	    margin-right:0px;

	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}


}   
.media-glow {
    position:absolute;
    z-index:2;
    @include box-shadow(#eb0 300px 50px 200px 150px);
    opacity:.2;
    width:800px;
}
.media-rule-top {
    clear:both;
    width:100%;
    height:2px;
    @include background-image(linear-gradient(left,#000 0%,#a0a 15%, #a0a 85%, #000 100%,#000)); 
}
.media-rule-bottom {
    clear:both;
    width:100%;
    height:1px;
    display:none;
    @include background-image(linear-gradient(left,#000 0%, #808 30%,#808 70%, #000 100%,#000)); 

}
#footer-container {
    position:relative;
    clear:both;
    text-align:center;
    vertical-align:bottom;
    font-weight:normal;
    opacity:.5;    
    height:150px;
    font-size:.8em;
    .rule {
	width:100%;
	height:1px;

	@include background-image(linear-gradient(left,#000 0%, #808 30%,#808 70%, #000 100%,#000)); 
	margin-bottom:20px;
    }
}

