$headerHeight:130px;

#breadcrumb {
    font-size:.8em;
    padding-top:5px;
    color:#c0c;
    margin-bottom:20px;
    margin-left:50px;
    width:100%;
    max-width:900px;
    
    .trail {
	font-weight:ligher;
	color:#ddd;
    }
}

#power-content {
    overflow:hidden;
    position:relative;
    margin:0 auto;
    max-width:900px;
    height:300px;
    font-style:italic;
    text-align:center;
    font-family:serif;
    .title {
	font-size:6em;
	}
    .subtitle {
	font-size:3em;
    }
    
}

#main-content {
    position:relative;
    margin-top:30px;
    max-width:1200px;
    height:100%;
    border-top:1px dotted #555;
	    
}
#main-content-background {
    position:absolute;
    z-index:1;
    height:100%;
    width:100%;
    background:#000;
    opacity:.3;
}
#main-content-foreground {
    position:absolute;
    padding:40px;
    z-index:2;
    width:100%;
    height:100%;
    color:#fff;

    .title {
	font-size:3em;
    }    
}

#services-navigation-container {
    position:relative;
    z-index:1;
    top:-80px;
    @include border-radius(10px);
    background-color:#303;
    clear:both;
    float:right;
    width:400px;
    left:-10px;
    height:360px;
    padding:20px;
    padding-top:80px;
    border:1px solid #c0c;
    opacity:1;
    @include transition(opacity .6s);
    font-size:.9em;
    p {
	text-align:center;
	margin-bottom:20px;
	font-size:1.2em;
	color:#d0d;
    }
    ul {
	margin:0 auto;
	width:380px;
	height:300px;
    }
    li {
	font-size:.8em;
	float:left;
	text-align:left;
	height:50px;
	width:100%;
	img {
	    float:left;
	    width:40px;
	    margin-top:5px;
	}
	h1 {
	    margin-left:60px;
	    margin-top:20px;
	}
	border-bottom:1px dotted #959;
    }
    
    &.hidden {
	opacity:0;
	top:-600px;
	@include transition(opacity .6s);
    }
}


.padme50 {
    padding:50px;
}
.center {
    text-align:center;
}

.more-info {
    margin-right:10px;
    margin-bottom:10px;
    font-family:$button;
    @include sassy-button("shiny", 5px, 16px, #2b2, #090);
}

#header-stripe {
    position:absolute;
    width:100%;
    height:$headerHeight;
    background:#000;
    opacity:.2;
    z-index:2;
}
#navigation-container {
    font-family: $heading;
    width:100%;
    height:41px;
    max-width:1100px;
    margin:0 auto;
    z-index:2;
    top:80px;
    font-size:1.5em;
    position:absolute;
    text-align:right;

    a {
	height:20px;
	@include transition(color 1s);
    }
    a,
    a:link,
    a:visited {
	color:#fff;	     
    }
    a.navon,
    a:hover {
	color:#f0f;
	text-decoration:none;
    }

    .main-navigation {
	position:relative;
	z-index:2;
	padding-right:100px;
	float:right;
	display:inline-block;
	width:270px;
	list-style:none;
	text-align: justify;
	-ms-text-justify: distribute-all-lines;
	text-justify: distribute-all-lines;
	
	li {
	    display: inline-block;
	    *display: inline;
	    zoom: 1;
	    padding:7px;	    	    
	}

	li.navon {
	    color:#f4f;
	    background-repeat:no-repeat;
	    background-position:center bottom;
	    background-image:url("../img/navigation-pointer.png");
	}

	&:after {   
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}
    }

}


.icon-envelop:before, .icon-phone:before, .icon-facebook:before, .icon-twitter:before {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	padding-right:10px;
	height:40px;
	
}
.icon-envelop:before {
	content: "\e000";
}
.icon-phone:before {
	content: "\e001";
}
.icon-facebook:before {
	content: "\e002";
}
.icon-twitter:before {
	content: "\e003";
}
#contact-container {
    position:absolute;
    text-align:right;
    width:100%;
    top:30px;
    opacity:.8;
    margin-right:0px;

    ul {
	text-align:left;
	display:inline-block;
