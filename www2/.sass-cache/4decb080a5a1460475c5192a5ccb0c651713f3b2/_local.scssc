	margin-left:20px;
	li {
	    padding:2px;
	    display:block;
	}
    }
}

body {
    background:#000;
    background-image:url("../img/texture2.jpg");
    background-repeat:repeat-x;
    font-size:1em;
    letter-spacing:0em;
    font-family:$text;
    font-weight:lighter;
    color:#fff;
}

#foreground-container {
    position:absolute;
    z-index:1;
    width:100%;
    height:100%;
}
#foreground {
    position:relative;
    height:100%;
    width:100%;
    max-width:1000px;
    margin:0 auto;
}

.page-container {
    margin:0 auto;
    width:100%;
    margin-top:10px;
}    
#header-container {
    position:relative;
    height:$headerHeight;
    max-width:1100px;
    width:100%;
    margin:0 auto;
}

#media-channels {
    width:900px;
    margin:0px;
    padding:0px;
    line-height:1.3em;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;

    li {
	width:255px;
	display:inline-block;
	zoom:1;
	vertical-align:top;
	text-align:center;
	margin:20px;
	margin-top:0px;
	h1 {
	    font-size:2em;
	    height:30px;
	}
	p {
	    padding-top:20px;
	    text-align:justify;
	    font-weight:lighter;
	    letter-spacing:.1em;
	    font-size:.8em;
	    color:#ddd;
	    }
	
    }
	&:after {   
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}

}
	
#media-content-container {

    list-style:none;
    height:300px;
    margin:0 auto;
    text-align: justify;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;
    padding-top:6px;
    padding-bottom:40px;
    width:100%;
    max-width:1100px;

    li:hover {
	background:#606;
    }	
    li {

	position:relative;
	margin:3px;
	display: inline-block;
	*display: inline;
	zoom: 1;
	min-width:290px;
	width:30%;

	.media-content {
	    position:absolute;
	    opacity:1;
	    background-color:#303;
	    height:270px;
	    @include border-radius(3px);
	    border:1px dotted #f0f;
	    h1 {
		@include border-radius(3px);
		padding:8px;
		background-color:#505;
		text-align:center;
		font-family:$heading;
		color:#f6f;
		font-size:2.5em;
		margin-bottom:10px;
	    }

	    p {
		padding:15px;
		text-align:center;
		font-weight:normal;
		font-family:$text;
		font-size:.8em;
		line-height:1.2em;
		letter-spacing:.1em;
	    }
 	    .media-top {
		height:180px;
	    }
	    .media-bottom {
		position:absolute;
		width:100%;
		bottom:0px;
		padding-top:10px;
		height:50px;
		background-color:#404;
		text-align:center;
		}
	    
	}
    }
	&:after {
	    margin-right:0px;

	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}


}   
.media-glow {
    position:absolute;
    z-index:2;
    @include box-shadow(#eb0 300px 50px 200px 150px);
    opacity:.2;
    width:800px;
}
.media-rule-top {
    clear:both;
    width:100%;
    height:2px;
    @include background-image(linear-gradient(left,#000 0%,#a0a 15%, #a0a 85%, #000 100%,#000)); 
}
.media-rule-bottom {
    clear:both;
    width:100%;
    height:1px;
    display:none;
    @include background-image(linear-gradient(left,#000 0%, #808 30%,#808 70%, #000 100%,#000)); 

}
#footer-container {
    position:relative;
    clear:both;
    text-align:center;
    vertical-align:bottom;
    font-weight:normal;
    opacity:.5;    
    height:150px;
