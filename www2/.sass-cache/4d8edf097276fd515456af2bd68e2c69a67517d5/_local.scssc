#services-navigation-container {
    position:relative;
    z-index:1;
    top:-80px;
    @include border-radius(10px);
    background-color:#303;
    clear:both;
    float:right;
    width:450px;
    left:-10px;
    height:360px;
    padding:20px;
    padding-top:80px;
    border:1px dotted #c0c;
    opacity:1;
    @include transition(opacity .6s);

    p {
	text-align:center;
	margin-bottom:20px;
	font-size:1.2em;
	color:#d0d;
    }
    ul {
	margin:0 auto;
	width:450px;
	height:300px;
    }
    li {
	font-size:.8em;
	float:left;
	text-align:left;
	height:50px;
	width:450px;
	img {
	    float:left;
	    width:40px;
	    margin-top:5px;
	}
	h1 {
	    margin-left:60px;
	    margin-top:20px;
	}
	border-bottom:1px dotted #959;
    }
    
    &.hidden {
	opacity:0;
	top:-600px;
	@include transition(opacity .6s);
    }
}


.padme50 {
    padding:50px;
}
.center {
    text-align:center;
}

.more-info {
    margin-right:10px;
    margin-bottom:10px;
    font-family:$button;
    @include sassy-button("shiny", 5px, 16px, #2b2, #090);
}

#header-stripe {
    position:absolute;
    width:4000px;
    margin-left:-1500px;
    height:120px;
    background:#000;
    opacity:.3;
    z-index:2;
}
#navigation-container {
    font-family: $heading;
    width:100%;
    height:41px;
    max-width:1100px;
    margin:0 auto;
    z-index:2;
    top:75px;
    font-size:1.5em;
    position:absolute;
    text-align:right;

    a {
	height:20px;
	@include transition(color 1s);
    }
    a,
    a:link,
    a:visited {
	color:#fff;	     
    }
    a.navon,
    a:hover {
	color:#f0f;
	text-decoration:none;
    }

    .main-navigation {
	position:relative;
	z-index:2;
	padding-right:60px;
	float:right;
	display:inline-block;
	width:400px;
	list-style:none;
	text-align: justify;
	-ms-text-justify: distribute-all-lines;
	text-justify: distribute-all-lines;
	
	li {
	    display: inline-block;
	    *display: inline;
	    zoom: 1;
	    padding:7px;	    	    
	}

	li.navon {
	    color:#f4f;
	    background-repeat:no-repeat;
	    background-position:center bottom;
	    background-image:url("../img/navigation-pointer.png");
	}

	&:after {   
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}
    }

}


.icon-envelop:before, .icon-phone:before, .icon-facebook:before, .icon-twitter:before {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	padding-right:10px;
	height:40px;
	
}
.icon-envelop:before {
	content: "\e000";
}
.icon-phone:before {
	content: "\e001";
}
.icon-facebook:before {
	content: "\e002";
}
.icon-twitter:before {
	content: "\e003";
}
#contact-container {
    position:absolute;
    text-align:right;
    width:100%;
    top:30px;
    opacity:.8;
    margin-right:0px;

    ul {
	text-align:left;
	display:inline-block;
	list-style:none;
	margin-left:20px;
	li {
	    padding:2px;
	    display:block;
	}
    }
}

body {
    background:#000;
    font-size:1em;
    letter-spacing:0em;
    font-family:$text;
    font-weight:lighter;
    color:#fff;
}

#foreground-container {
    position:absolute;
    margin-top:30px;
    z-index:1;
    width:100%;
    height:100%;
}
#foreground {
    position:relative;
    height:100%;
    width:100%;
    max-width:1100px;
    margin:0 auto;
}

.page-container {
    margin:0 auto;
    width:100%;
    margin-top:10px;
}    
#header-container {
    position:relative;
    height:130px;
    max-width:1100px;
    width:100%;
    margin:0 auto;
}

	
#media-content-container {

    list-style:none;
    height:300px;
    margin:0 auto;
    text-align: justify;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;
    padding-top:6px;
    padding-bottom:40px;
    width:100%;
    max-width:1100px;

    li:hover {
	background:#606;
    }	
    li {

	position:relative;
	margin:3px;
	display: inline-block;
	*display: inline;
	zoom: 1;
	min-width:290px;
	width:30%;

	.media-content {
	    position:absolute;
	    opacity:1;
	    background-color:#303;
	    height:270px;
	    @include border-radius(3px);
	    border:1px dotted #f0f;
	    h1 {
		@include border-radius(3px);
		padding:8px;
		background-color:#505;
		text-align:center;
		font-family:$heading;
		color:#f6f;
		font-size:2.5em;
		margin-bottom:10px;
	    }

	    p {
		padding:15px;
		text-align:center;
		font-weight:normal;
		font-family:$text;
		font-size:.8em;
		line-height:1.2em;
		letter-spacing:.1em;
	    }
 	    .media-top {
		height:180px;
	    }
	    .media-bottom {
		position:absolute;
		width:100%;
		bottom:0px;
		padding-top:10px;
		height:50px;
		background-color:#404;
		text-align:center;
		}
	    
	}
    }
	&:after {
	    margin-right:0px;

	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}


}   
.media-glow {
    position:absolute;
    z-index:2;
    @include box-shadow(#eb0 300px 50px 200px 150px);
    opacity:.2;
    width:800px;
}
.media-rule-top {
    clear:both;
    width:100%;
    height:2px;
    @include background-image(linear-gradient(left,#000 0%,#a0a 15%, #a0a 85%, #000 100%,#000)); 
}
.media-rule-bottom {
    clear:both;
    width:100%;
    height:1px;
    display:none;
    @include background-image(linear-gradient(left,#000 0%, #808 30%,#808 70%, #000 100%,#000)); 

}
#footer-container {
    position:relative;
    clear:both;
    text-align:center;
    vertical-align:bottom;
    font-weight:normal;
    opacity:.5;    
    height:150px;
    font-size:.8em;
    .rule {
	width:100%;
	height:1px;

	@include background-image(linear-gradient(left,#000 0%, #808 30%,#808 70%, #000 100%,#000)); 
	margin-bottom:20px;
    }
}

.displaynone,
.display-off {
    display:none;
}

#home-button-container {
    background-image:url("../img/backsun.png");
    background-position:center bottom;
    background-repeat:no-repeat;

    span {
	position:absolute;
	height:0px;
	width:100%;
	opacity:.4;
	@include box-shadow(#fff 0px 100px 200px 30px);
	
    }

    ul {
	padding-left:10px;
	padding-right:10px;
	margin:0 auto;
	max-width:1100px;
	list-style:none;
	text-align: justify;
	-ms-text-justify: distribute-all-lines;
	text-justify: distribute-all-lines;
	font-size:.8em;

	li {
	    display: inline-block;
	    *display: inline;
	    zoom: 1;
	    padding-bottom:30px;
	}

	li.navon {
	    color:#f4f;
	    background-repeat:no-repeat;
	    background-position:center bottom;
	    background-image:url("../img/navigation-pointer.png");
	}

	&:after {
	    content: '';
	    width: 100%;
	    display: inline-block;
	    font-size: 0;
	}
    }
}

.home-button-container {
    float:left;
    cursor:pointer;

}
.home-button {
    height:150px;
    width:320px;
    position:relative;
}    

.home-button-content {

    position:absolute;
    z-index:2;

    img {
	padding-top:32px;
	padding-left:16px;
	width:90px;
    }

    h1 {
	position:absolute;
	margin-left:95px;
	font-size:2.4em;
	letter-spacing:0em;
	top:72px;
	width:225px;
	text-align:center;
	font-weight:lighter;
	font-family:$text;
    }

    h2 {
	width:225px;
	position:absolute;
	margin-left:95px;
	font-size:1.3em;
	top:50px;
	text-align:center;
	color:#fff;
	letter-spacing:0em;
    }
}
.home-button-hover,
.home-button-hover.dormant,
.home-button-border {
    z-index:1;
    position:absolute;
    border:1px solid #fff;
    @include border-radius(11px);
    opacity:.3;
}
.home-button-hover {
    background:#f0f;
    opacity:.6;
    @include transition(opacity .6s);
}
.home-button-hover.dormant {
    opacity:0;
    background:#f0f;
}

#mailing-list {
    form {
	margin:0px;
	padding:0px;
	width:100%;
	text-align:center;
    }
    input {
	padding:8px;
	background-color:#fcf;
	border:1px solid #909;
	margin-top:10px;
	width:180px;
	font-size:.9em;
	@include border-radius(5px);
    }
    
}
.signup-button {
	@include sassy-button("shiny", 8px, 15px, #f0f, #a0a);
	font-size:1em;
	font-family:$button;
	padding:6px;
    }

#from-our-blog,
#latest-news {
    p {
	padding-bottom:10px;
    }
}
a,
a:link,
a:visited {
    color:#f9f;	     
    text-decoration:none;
}
a:hover {
    color:#0f0;
    text-decoration:none;
}


/*************** ABOUT US ****************/
#aboutus {
    width:100%;
    height:800px;
    .power-image {
	text-align:right;
	width:100%;
	top:80px;
    }
    p {
	padding-left:50px;
	width:600px;
    }
}		
/**************** WEB DESIGN **************/
#mobile,
#webdesign {
    width:100%;
    height:800px;
    .power-image {
	text-align:center;
	
	img {
	    width:600px;
	    }
    }
    .power-message {
	text-align:center;
	width:100%;
	font-size:1.5em;
	top:200px;
	padding:0px;
    }
    p {
	padding-left:150px;
	padding-right:150px;
	text-align:left;
	font-size:.7em;

    }
}
    
#invitation {
    text-align:center;
    width:840px;
    margin:0 auto;

    .invite-top {

	@include border-radius(10px);
	margin:0 auto;
	width:100%;
	background-color:#999;
	padding-top:70px;
	background-image:url("../img/201d.png");
	background-repeat:no-repeat;
	background-position:700px 40px;

	h1 {
	    margin:20px;
	    margin-top:0px;
	    padding-bottom:10px;
	    border-bottom:1px solid #fff;
	    text-align:left;
	    font-family:"Lobster Two";
	    font-style:italic;
	}
	h2.power-title {
	    font-family:"Lobster Two";
	    font-style:italic;
	    padding-top:20px;
	    text-align:center;
	    color:#808;
	    font-size:3em;
	    margin-bottom:40px;
	}

	.invite-middle {
	    padding-top:60px;
	    padding-bottom:60px;
	    width:100%;
	    margin:0 auto;
	    background-color:#fff;
	    margin-bottom:20px;

	    p {
		padding:70px;
		padding-top:10px;
		padding-bottom:10px;
		text-align:center;
		font-size:.9em;
		color:#777;
		letter-spacing:0em;
		line-height:1.2em;
		font-weight:normal;
	    }
	}

	.rsvp-details {
	    letter-spacing:0em;
	    padding:6px;
	    color:#555;
	    font-size:1.2em;
	}

	h3 {
	    font-size:1.8em;
	    padding-bottom:20px;
	}
	.invite-footer {
	    background:#606;
	    margin-top:20px;
	    border-top:1px solid #fff;
	    padding:30px;
	    padding-left:160px;
	    padding-right:160px;
	    letter-spacing:0em;
	    clear:both;
	    text-align:center;
	    .social-links {
		width:140px;

		margin:30px auto;
		height:50px;
		
		li {
		    
		    width:100px;
		    height:20px;
		    overflow:hidden;
		    margin-left:20px;
		    margin-right:20px;
		    background-image:url("../img/icons-social.png");
		    background-repeat:none;
		    float:left;
		    display:inline-block;

		}
	    }
	}
    }
    
